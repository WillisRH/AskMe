<!DOCTYPE html>
<img
  src="data:image/png;base64,"
  style="
    border-radius: 125px;
    display: block;
    margin: 100px auto;
    width: 100px;
    height: 100px;
  "
/>
<a href="" style="text-align: center">Upload Image</a>
<h2 style="text-align: center">Selamat (siang/pagi/malam) <%=username %>!</h2>

<style>
  * {
    text-align: center;
  }

  table {
    margin: 0 auto;
    font-size: 25px;
    text-align: center;
  }

  h4 {
    margin-bottom: 50px;
  }
</style>

<header>
  {
  <h4>
    Email: <%= email %>, <br />
    Username: <%= username %>, <br />
    Token (debug): <%= token %>, <br />
    AccountID: <%= id %>, <br />
    }
  </h4>

  <% if (questions.length < 3 || id == '94243') { %>
  <table>
    <thead>
      <tr>
        <th>Question ID</th>
        <th>Question Title</th>
      </tr>
    </thead>
    <tbody>
      <% questions.forEach(function(question) { %>
      <tr>
        <td>
          <a href="/home/askme/admin/<%= question.questionid %>"
            ><%= question.questionid %></a
          >
        </td>
        <td>
          <a href="/home/askme/admin/<%= question.questionid %>"
            ><%= question.questiontitle %></a
          >
        </td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  <a
    href="/home/askme/admin/<%= token %>/profile/createquestions"
    style="margin: 0 auto; text-align: center"
    >+ Create your question session!</a
  >

  <% } else { %>
  <table>
    <thead>
      <tr>
        <th>Question ID</th>
        <th>Question Title</th>
      </tr>
    </thead>
    <tbody>
      <% questions.forEach(function(question) { %>
      <tr>
        <td>
          <a href="/home/askme/admin/<%= question.questionid %>"
            ><%= question.questionid %></a
          >
        </td>
        <td>
          <a href="/home/askme/admin/<%= question.questionid %>"
            ><%= question.questiontitle %></a
          >
        </td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  <% } %>
</header>
